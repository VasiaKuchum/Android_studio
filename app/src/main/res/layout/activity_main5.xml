        <TextView
            android:id="@+id/textView45"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Salt (1 tsp in the dough and 1 tsp in the sauce) - 2 tsp"
            android:textColor="#020000" />
